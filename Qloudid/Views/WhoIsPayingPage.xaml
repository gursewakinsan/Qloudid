												FontSize="14"
												HorizontalTextAlignment="Start"
												LineBreakMode="TailTruncation"
												MaxLines="1"
												Text="{Binding SubHeadingAddress}"
												TextColor="White"
												VerticalTextAlignment="Center" />
										</StackLayout>

										<Button
											Grid.Column="1"
											Padding="0"
											BackgroundColor="#466285"
											CornerRadius="20"
											FontFamily="{StaticResource MaterialFontFamily}"
											HeightRequest="40"
											HorizontalOptions="CenterAndExpand"
											Text="{x:Static fontawesome:QloudidAppFlatIcons.Plus}"
											TextColor="White"
											VerticalOptions="CenterAndExpand"
											WidthRequest="40" />
									</Grid>
								</Frame>

								<Frame
									Margin="0,0,0,20"
									BackgroundColor="#242A37"
									BorderColor="#242A37"
									CornerRadius="8"
									IsClippedToBounds="True"
									IsVisible="{Binding IsBusiness}">
									<Grid>
										<Grid.ColumnDefinitions>
											<ColumnDefinition Width="*" />
											<ColumnDefinition Width="40" />
										</Grid.ColumnDefinitions>
										<StackLayout
											Grid.Column="0"
											Spacing="1"
											VerticalOptions="CenterAndExpand">
											<Label
												FontFamily="PoppinsRegularFontFamily"
												FontSize="14"
												HorizontalTextAlignment="Start"
												LineBreakMode="TailTruncation"
												MaxLines="1"
												Text="{Binding NameOnHouse}"
												TextColor="White"
												VerticalTextAlignment="Center" />

											<Label
												FontFamily="PoppinsRegularFontFamily"
												FontSize="14"
												HorizontalTextAlignment="Start"
												LineBreakMode="TailTruncation"
												MaxLines="1"
												Text="{Binding UserName}"
												TextColor="White"
												VerticalTextAlignment="Center" />

											<Label
												FontFamily="PoppinsRegularFontFamily"
												FontSize="14"
												HorizontalTextAlignment="Start"
												LineBreakMode="TailTruncation"
												MaxLines="1"
												Text="{Binding HeadingAddress}"
												TextColor="White"
												VerticalTextAlignment="Center" />

											<Label
												FontFamily="PoppinsRegularFontFamily"
												FontSize="14"
												HorizontalTextAlignment="Start"
												LineBreakMode="TailTruncation"
												MaxLines="1"
												Text="{Binding SubHeadingAddress}"
												TextColor="White"
												VerticalTextAlignment="Center" />
										</StackLayout>

										<Button
											Grid.Column="1"
											Padding="0"
											BackgroundColor="#466285"
											CornerRadius="20"
											FontFamily="{StaticResource MaterialFontFamily}"
											HeightRequest="40"
											HorizontalOptions="CenterAndExpand"
											Text="{x:Static fontawesome:QloudidAppFlatIcons.Plus}"
											TextColor="White"
											VerticalOptions="CenterAndExpand"
											WidthRequest="40" />
									</Grid>
								</Frame>
							</StackLayout>
						</ViewCell>
					</DataTemplate>
				</ListView.ItemTemplate>
			</ListView>
			<BoxView
				Grid.Row="9"
				BackgroundColor="#242A37"
				CornerRadius="12" />

			<Frame
				Grid.Row="9"
				Margin="2"
				Padding="0"
				BackgroundColor="#181A1F"
				CornerRadius="12"
				HasShadow="False"
				IsClippedToBounds="True">

				<StackLayout
					Grid.Row="9"
					HorizontalOptions="CenterAndExpand"
					Orientation="Horizontal"
					Spacing="8"
					VerticalOptions="CenterAndExpand">

					<Label
						FontFamily="PoppinsSemiBoldFontFamily"
						FontSize="16"
						HorizontalTextAlignment="Center"
						Text="Add card"
						TextColor="White"
						VerticalTextAlignment="Center" />

					<Label
						FontFamily="{StaticResource MaterialFontFamily}"
						FontSize="20"
						HorizontalTextAlignment="Center"
						Text="{x:Static fontawesome:QloudidAppFlatIcons.ArrowRight}"
						TextColor="White"
						VerticalTextAlignment="Center" />
				</StackLayout>
			</Frame>
		</Grid>
	</Grid>


