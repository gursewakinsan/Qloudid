	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:controls="clr-namespace:Qloudid.Controls"
	xmlns:fontawesome="clr-namespace:Qloudid.Helper"
	xmlns:ios="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core"
	ios:Page.UseSafeArea="True"
	BackgroundColor="#181A1F"
	NavigationPage.HasNavigationBar="False">
	<Grid
		Margin="0,22,0,5"
		ColumnSpacing="0"
		RowSpacing="0">
		<Grid.RowDefinitions>
			<RowDefinition Height="40" />
			<RowDefinition Height="*" />
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="28" />
			<ColumnDefinition Width="40" />
			<ColumnDefinition Width="*" />
			<ColumnDefinition Width="40" />
			<ColumnDefinition Width="28" />
		</Grid.ColumnDefinitions>

		<Frame
			Grid.Row="0"
			Grid.Column="1"
			Padding="0"
			BackgroundColor="Transparent"
			BorderColor="White"
			CornerRadius="8"
			IsClippedToBounds="True"
			Opacity="0.2" />

		<Button
			Grid.Row="0"
			Grid.Column="1"
			Padding="0"
			BackgroundColor="Transparent"
			Command="{Binding BackCommand}"
			FontFamily="{StaticResource MaterialFontFamily}"
			FontSize="25"
			Text="{x:Static fontawesome:QloudidAppFlatIcons.ChevronLeft}"
			TextColor="White" />

		<Image
			Grid.Row="0"
			Grid.Column="3"
			Aspect="AspectFit"
			BackgroundColor="Transparent"
			HeightRequest="40"
			HorizontalOptions="EndAndExpand"
			Source="iconSmallLogo.png"
			VerticalOptions="CenterAndExpand"
			WidthRequest="40" />

		<Grid
			Grid.Row="1"
			Grid.Column="1"
			Grid.ColumnSpan="3"
			Margin="0,31,0,0"
			ColumnSpacing="0"
			RowSpacing="0">
			<Grid.RowDefinitions>
				<RowDefinition Height="48" />
				<RowDefinition Height="16" />
				<RowDefinition Height="26" />
				<RowDefinition Height="8" />
				<RowDefinition Height="1" />
				<RowDefinition Height="41" />
				<RowDefinition Height="36" />
				<RowDefinition Height="20" />
				<RowDefinition Height="*" />
				<RowDefinition Height="56" />
			</Grid.RowDefinitions>
			<Label
				Grid.Row="0"
				FontAttributes="Bold"
				FontFamily="PoppinsBoldFontFamily"
				FontSize="40"
				HorizontalTextAlignment="Start"
				Text="Pay with..."
				TextColor="White"
				VerticalTextAlignment="Center" />

			<Label
				Grid.Row="2"
				FontFamily="PoppinsRegularFontFamily"
				FontSize="16"
				HorizontalTextAlignment="Start"
				LineBreakMode="TailTruncation"
				MaxLines="1"
				Text="Select or add a new invocing address"
				TextColor="White"
				VerticalTextAlignment="Center" />

			<BoxView Grid.Row="4" BackgroundColor="#2A2A31" />

			<StackLayout
				Grid.Row="6"
				HorizontalOptions="StartAndExpand"
				Orientation="Horizontal"
				Spacing="20">
				<controls:CustomButton
					Margin="0"
					BackgroundColor="{Binding BtnPersonalBg}"
					BorderColor="{Binding BtnPersonalBg}"
					Command="{Binding ShowPersonalAddressesCommand}"
					CornerRadius="12"
					FontFamily="PoppinsSemiBoldFontFamily"
					FontSize="12"
					Text="Personal"
					TextColor="White"
					WidthRequest="94" />

				<controls:CustomButton
					Margin="0"
					BackgroundColor="{Binding BtnBusinessBg}"
					BorderColor="{Binding BtnBusinessBg}"
					Command="{Binding ShowBusinessAddressesCommand}"
					CornerRadius="12"
					FontFamily="PoppinsSemiBoldFontFamily"
					FontSize="12"
					Text="Business"
					TextColor="White"
					WidthRequest="94" />
			</StackLayout>

			<ListView
				x:Name="listInvoiceAddress"
				Grid.Row="8"
				BackgroundColor="#181A1F"
				HasUnevenRows="true"
				ItemTapped="OnInvoiceAddressItemTapped"
				ItemsSource="{Binding ListOfInvoiceAddress}"
				SeparatorVisibility="None"
				VerticalOptions="FillAndExpand"
				VerticalScrollBarVisibility="Never">
