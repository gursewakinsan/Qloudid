                                </Grid>

                                <Grid 
                                    WidthRequest="278" 
                                    IsVisible="{Binding Address.IsWiFiUpdated, Converter={StaticResource InverseBoolConverter}}">
                                    <Grid.GestureRecognizers>
                                        <TapGestureRecognizer Command="{Binding EditWiFiCommand}"/>
                                    </Grid.GestureRecognizers>
                                    <Frame 
                                        Grid.Row="0"
										Padding="10"
										IsClippedToBounds="True" 
										HasShadow="False"
										CornerRadius="16"
										BackgroundColor="#242A37">
                                        <Frame.GestureRecognizers>
                                            <TapGestureRecognizer Command="{Binding EditWiFiCommand}"/>
                                        </Frame.GestureRecognizers>
                                        <Grid RowSpacing="0" ColumnSpacing="7" Margin="11,26,0,40">
                                            <Grid.GestureRecognizers>
                                                <TapGestureRecognizer Command="{Binding EditWiFiCommand}"/>
                                            </Grid.GestureRecognizers>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="87"/>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="40"/>
                                                <RowDefinition Height="20"/>
                                                <RowDefinition Height="24"/>
