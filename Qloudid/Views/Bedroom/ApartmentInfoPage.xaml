                                                Grid.Column="1"
												FontFamily="PoppinsRegularFontFamily"
												FontSize="16"
												Text="Other rooms"
												TextColor="White" 
                                                Opacity="0.60"
                                                VerticalTextAlignment="Center"
                                                HorizontalTextAlignment="Start">
                                                <Label.GestureRecognizers>
                                                    <TapGestureRecognizer Command="{Binding GoToDoApartmentPageCommand}"/>
